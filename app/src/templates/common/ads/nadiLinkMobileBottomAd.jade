script.
    function setupPopupAdUnitClick(){
        $("#recommended-wrapper").attr("style","max-height:300px;background: #fff;border: 2px solid #000; padding: 5px;");
        $("#recommended-wrapper ul").addClass("hide");
        $("#recommended-wrapper .pop_owl").addClass("hide");
    }
    
    if("!{env.isMobile}"==="false"){
        console.log("hiding it");
        setTimeout(function(){$("#fixed-bottom-content").addClass("hide");},1);
    }  else {
        $("#fixed-bottom-content").removeClass("hide");
        $(".recommended-close").on("click",function(){
            $("#fixed-bottom-content").addClass("hide");
        });
        
        var e =  $(".recom-image-slider-vertical-listing");
        e.owlCarousel({
                 autoPlay: 3000,
                 navigation: false,
                 pagination: false,
                 nav:false

                 
             });
        $(".pop_owl.next").click(function(){
            e.trigger('owl.next');
          })
        $(".pop_owl.prev").click(function(){
            e.trigger('owl.prev');
        })
        
             
    }
    