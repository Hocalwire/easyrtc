body {
                overflow-y:scroll;
            }
            amp-sidebar {
              width: 250px;
              padding-right: 10px;
            }
            .amp-sidebar-image {
              line-height: 100px;
              vertical-align:middle;
            }
            .amp-close-image {
               top: 15px;
               left: 225px;
               cursor: pointer;
            }
            li {
              margin-bottom: 10px;
              list-style: none;
            }
            button {
              margin-left: 20px;
            }
            .header {
                background: #fff;
                height: 62px;
            }
            .header .topHeader {
                height: 40px;
                margin-top: 10px;
                margin-bottom: 20px;
                position: relative;
            }
            .leftmenuSec {
                float: left;
                width: 12%;
                height: 30px;
            }
            .openmenu {
                border: 0;
                width: 60px;
                height: 40px;
                margin-left: 10px;
            }
            .openmenu.menu_bar{
                background:red;
            }
            #sidebar .sub-cats,#sidebar .sub-cats a {
                font-size:12px;
                margin-left:10px;
                padding:2px;
            }
            .amp-close-image {
                margin-left:-5px;
            }
            .pull-right {
                float:right;
                margin-right:10px;
            }
            h1.story_title {
                font-size:25px;
                text-align:center;
            }
            .story-content {
                padding: 5px 20px 0;
            }
            .story-content .author-name, .story-content .story-time {
                color: #999;
            }
            .amp-carousel-button{
                visibility:visible ;
                opacity:1 ;
            }
            .newsSocialIcons li a {
                color: #FFF;
            }
            .newsSocialIcons li a:hover {
                text-decoration: none ;
            }
            .newsSocialIcons li a i {
                margin-right: 4px;
            }

            .newsSocialIcons {
                display: inline-block;
                text-align: right;
                
            }
            .newsSocialIcons a {
                padding-right:5px;
                display: inline-block;
            }
            .newsSocialIcons li {
                list-style: none ;
                width: 25px;
                height: 25px;
                text-decoration: none;
                font-family: Oswald ;
                text-transform: uppercase;
                background: transparent;
                opacity: 1;
                line-height: 30px;
                padding: 0;
                margin: 0 3px;
                float:left;
            }

            .newsSocialIcons li a {
                color: #FFF;
                height:15px;
            }

            .newsSocialIcons li a:hover {
                text-decoration: none ;
            }

            .newsSocialIcons li a i {
                margin-right: 4px;
            }

            .newsSocialIcons li.facebook {
                border: 0px solid #314b83;
                background-color: #4769A5;
            }

            .newsSocialIcons li.twitter {
                border: 0px solid #0092ba;
                background-color: #65CCEF;
            }

            .newsSocialIcons li.googleplus {
                border: 0px solid #ab2b1d;
                background-color: #bf3727;
            }
            .newsSocialIcons li.gplus {
                border: 0px solid #ab2b1d;
                background-color: #bf3727;
            }
            .newsSocialIcons li.linkedin {
                border: 0px solid #278cc0;
                background-color: #2ba3e1;
            }

            .newsSocialIcons li.tumblr {
                border: 0px solid #2C3C4C;
                background-color: #34495e;
            }

            .newsSocialIcons li.pinterest {
                border: 0px solid #ae1319;
                background-color: #cd252b;
            }

            .newsSocialIcons li.mail {
                border: 0px solid #18AE91;
                background-color: #1abc9c;
            }
            .newsSocialIcons li.youtube {
                border: 0px solid #e62117;
                background-color: #e62117;
            }

            .newsSocialIcons li img {
              vertical-align: top;
            }
            .newsSocialIcons ul{
              border-radius: 5px;
              padding:5px;
              z-index:10;
              width: 230px;
              float: left;
              clear: both;
            }
            .newsSocialIcons li.whatsapp, .sticky li.whatsapp {
                border: 0 solid #19d639;
                background-color: #24bc3d;
            }
            .image_caption{
              margin-top: 10px;
              display:block;
              padding: 3px 10px;
              background: #eee;
              color:#747474;
              font-size: 13px;
            }
            .pagi_wrap {
                
                position: relative;
                
            }
            .pagi_wrap ul {
                margin: auto;
                display: block;
                text-align: center;
                width: 615px;
                position: relative;
            }
            .pagi_wrap ul li {
                display: inline-block;
                margin: 0px 15px;
                text-align: center;
                position: relative;
            }
            .pagi_wrap ul li.active a {
                cursor: default;
            }
            .pagi_wrap ul li span {
                color: #333;
                vertical-align: middle;
                display: inline-block;
                font-size: 20px;
                line-height: 45px;
                width: 45px;
                height: 45px;
                background: #dfebf9;
                text-align: center;
                border-radius: 50%;
            }
            .pagi_wrap ul li.active span {
                background: #ccc;
            }
            #footer .footer-content {
                background-color: #f5f5f5;
                border-top: 1px solid #fff;
                margin: 0 auto;
                padding: 0 0 2px;
                position: relative;
                
            }
            #footer .footer-content .content-item {
                margin: 0 auto;
                display: inline;
            }
            p.copyright {
                display: inline;
            }
            #footer .powered-by-hocalwire {
                font-size: 12px;
            }
            p.powered-by-hocalwire {
                margin-right: 10px;
            }
            .static-links.dark-bg {
                background: #0f0f0f;
                min-height: 40px;
            }
            .follow-us-block {
                padding-top: 5px;
                text-align: left;
                color: #fff;
                padding-bottom:2px;
            }
            #footer .footer-content .content-item {
                margin: 0 auto;
                display: inline;
            }
            #footer .footer-content .content-item {
                padding: 0;
                position: relative;
                
            }
            #footer .footer-content .social_nav {
                    position: absolute;
                    padding: 0px;
                    padding-left: 10px;
                    margin-top: 5px;
            }
            #footer .follow-label {
                float:left;
            }
            #footer .footer-content .social_nav li {
                float:right
            }
            #footer .footer-content .follow-us-links.newsSocialIcons {
                padding:5px;
            }
            #footer .footer-legals {
                height: 15px;
                padding-top: 5px;
            }
            #footer .footer-legals li.nav-legals{
                position: relative;
                padding-right: 30px;
            }
            #footer .footer-legals li {
                float: left;
                margin-right: 10px;
            }
            #footer .footer-legals li a {
                color: #000;
                text-decoration: none;
            }
            .follow-us-links.newsSocialIcons {
                float:left;
            }
