doctypemobile
html(dir='ltr')
  head
    script.
        window.ignorePageViewTracking=true;
    -meta.image="https://hocalwire.com/images/ads/image2.jpg"
    -meta.title="पाए मोटापे और डायबिटीज से छुटकारा , सिर्फ आयुर्वेदिक इलाज, घरेलु नुस्खों से, व्हाट्सप्प करे"
    -meta.description="पाए मोटापे और डायबिटीज से छुटकारा , सिर्फ आयुर्वेदिक इलाज, घरेलु नुस्खों से, व्हाट्सप्प करे"
    include ../headSectionTop
body
    block content
        .outer.nadi-ad.nadi-inline
            .level_1
                .hocal_row
                    div.video-player-wrapper.fallback-ad
                        

    script.
        function getParameterByName(name, url) {
            if (!url) url = window.location.href;
            name = name.replace(/[\[\]]/g, "\\$&");
            var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
                results = regex.exec(url);
            if (!results) return null;
            if (!results[2]) return '';
            return decodeURIComponent(results[2].replace(/\+/g, " "));
        }
        var message = getParameterByName("mesage_to_send");
        var message_from = getParameterByName("message_from");
        if(message_from){
            message_from=decodeURIComponent(message_from);
        }
        var text;
        if(message) {
            text = "https://api.whatsapp.com/send?phone="+getParameterByName("phone")+"&text=!{encodeURIComponent(message)}";
        } else {
           text = "https://api.whatsapp.com/send?phone="+getParameterByName("phone")+"&text=!{encodeURIComponent(' whatsapp द्वारा हमारी टीम को समपर्क करने के लिए धन्यवाद, स्वस्था विचार आयुर्वेदाचार्य आपका स्वागत करता है, आप अपनी समस्या बताये और वैद्य जी आपको उचित सलाह और उपचार बतायेगे.\n आयुर्वेद आपकी हर समस्या का इलाज करेगा.')}";
        } 
        if(message_from){
            text = text+' आपके द्वारा दिए गयी जानकारी - ' + message_from;
        }
        if("!{env.isBot}"==true){

        } else {
            window.location=text;
        }
        