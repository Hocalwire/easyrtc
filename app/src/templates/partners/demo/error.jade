extends ../../theme_fox/layout
block content
    div.wrapper
        include ../../theme_fox/partials/_headerWrapper

        //- div#top_full_carousel
        //-   -if(data['top_full_carousel'] && data['top_full_carousel'].templateData && data['top_full_carousel'].templateData.mixinName){
        //-         +#{data['top_full_carousel'].templateData.mixinName}(data['top_full_carousel'])
        //-   -}
        div(class="main-content")
            div(class="container")
                div(class="row")
                    div(class="col-md-8")
                        .cp-single-post
    
                            include ../../theme_fox/partials/_websiteCurrentTags
                            include ../../theme_fox/partials/_breadcrumb
                            #left_level_1.left_level_1
                            div(class="cp-post-content")
                                h1 Error Occured
                            #left_level_2.right_level_2
                            .details-content-story
                                    p   We&apos;re sorry, but looks like something went wrong. Now we know about it we&apos;ll try to fix it.  
                                    span    Goto
                                    a(href="/")  Home Page
                                
                                div.social
                                    .entry-box
                                        -if(env.partnerData.facebookUrl || env.partnerData.twitterUrl){
                                            ul.social-link.clearfix
                                              li Like Us:
                                              -if(env.partnerData.facebookUrl){
                                                li
                                                  div(class="fb-like", data-href="#{env.partnerData.facebookUrl}", data-layout="button_count" data-action="like", data-show-faces="false", data-share="true")
                                              -}
                                              -if(env.partnerData.twitterUrl){
                                                li
                                                  a(class="twitter-follow-button", href="#{env.partnerData.twitterUrl}") Follow
                                              -}
                                          
                                        -}
                            #left_level_3.right_level_3
                            
                            #left_level_4.right_level_4
                                  
                    div(class="col-md-4")
                        .sidebar.side-bar.right-sidebar
                            include ../../theme_fox/partials/_staticRightSection
                div(class="row")
                    div(class="col-lg-12 col-md-12 col-sm-12")
                        #level_bottom_full
                            -if(data['level_bottom_full'] && data['level_bottom_full'].templateData && data['level_bottom_full'].templateData.mixinName){
                                +#{data['level_bottom_full'].templateData.mixinName}(data['level_bottom_full'])
                            -}
        include ../../theme_fox/partials/_footer