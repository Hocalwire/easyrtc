include ../../theme_fox/mixin/_megaMenuWithChilds
include ../../theme_fox/mixin/_megaMenuWithoutChilds
include ../../theme_fox/mixin/_newsFullCarousel
include ../../theme_fox/mixin/_newsFullCarouselWithDescription
include ../../theme_fox/mixin/_newsGrid
include ../../theme_fox/mixin/_newsGridDouble
include ../../theme_fox/mixin/_newsGridSmall
include ../../theme_fox/mixin/_newsLinks
include ../../theme_fox/mixin/_newsListing
